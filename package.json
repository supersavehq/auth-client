{
  "name": "@supersave/auth-client",
  "version": "1.0.3",
  "author": "daangemist",
  "type": "module",
  "keywords": [
    "supersave",
    "api",
    "client",
    "auth"
  ],
  "homepage": "https://github.com/supersavehq/auth-client",
  "issues": "https://github.com/supersavehq/auth-client/issues",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/auth-client.cjs",
    "default": "./dist/auth-client.modern.js"
  },
  "main": "./dist/auth-client.cjs",
  "module": "./dist/auth-client.module.js",
  "unpkg": "./dist/auth-client.umd.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext=.ts ./src ./tests",
    "lint:fix": "eslint --ext=.ts --fix ./src ./tests",
    "format": "prettier --check ./src ./tests",
    "format:fix": "prettier --write ./src ./tests"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:unicorn/recommended"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": "error"
    }
  },
  "devDependencies": {
    "@supersave/auth": "^1.0.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^28.1.6",
    "@types/jest-when": "^3.5.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "axios": "^1.3.2",
    "eslint": "^8.20.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "express": "^4.17.3",
    "jest": "^28.1.3",
    "jest-when": "^3.5.1",
    "microbundle": "^0.15.0",
    "prettier": "^2.7.1",
    "supersave": "^0.12.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
